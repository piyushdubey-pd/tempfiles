<!-- ******************************************  FORM  ************************************  -->
<section>

<!------ Include the above in your HEAD tag ---------->

<div class="container contact offset-md-2">
	<div class="row">
		
		<div class="col-md-9">
		    
			<div class="contact-form">
	 


	           <form action="NotifyCreate" method="post"> 
			    
				<div class="form-group" >
				  <label class="control-label col-sm-2" for="EventId">Event ID:</label>
				  <div class="col-sm-3"  >          
					<input type="text" class="form-control"  placeholder="" name="EventId" value="">
				  </div>
				</div>
				
				
				<div class="form-group">
				   <label class="control-label col-sm-4" for="EventDetails">Event Notification:</label> 
				  <div class="col-sm-10">
					<textarea class="form-control" name="EventNotify" value="" rows="2"></textarea>
				  </div>
				</div>
				
				
				
				<p style="color:red;"><%=NErrtxt%></p>
				
				<div class="form-group">        
				  <div class="col-sm-offset-2 col-sm-10">
					<button type="submit"  class="btn btn-default">CREATE</button>
				  </div>
				</div>
				
				</form> 
				
			</div>
		</div>
        

	</div>

     <div class="NSuccessTxt">
    <span><%=SuNotifyTxt%></span>
    </div>

</div>






